SELECT 
  COALESCE(teprjtask_cust_id,-1) AS cust_id,
  COALESCE(teprjtask_curr_id,-1) AS curr_id,
  COALESCE(teprjtask_item_id,-1) AS item_id,
  *
FROM te.teprjtask
WHERE (teprjtask_prjtask_id=<? value("prjtask_id") ?>);
